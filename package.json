{
  "name": "sources-ui",
  "version": "1.1.0",
  "private": false,
  "insights": {
    "appname": "sources"
  },
  "dependencies": {
    "@data-driven-forms/pf4-component-mapper": "^1.27.6",
    "@data-driven-forms/react-form-renderer": "^1.27.6",
    "@patternfly/react-core": "^3.136.14",
    "@patternfly/react-icons": "^3.14.43",
    "@patternfly/react-table": "^2.25.6",
    "@redhat-cloud-services/frontend-components": "1.0.2",
    "@redhat-cloud-services/frontend-components-notifications": "1.0.1",
    "@redhat-cloud-services/frontend-components-sources": "1.0.2",
    "@redhat-cloud-services/frontend-components-utilities": "1.0.0",
    "@sentry/minimal": "5.12.0",
    "axios": "0.19.2",
    "deep-object-diff": "^1.1.0",
    "lodash": "^4.17.15",
    "react": "^16.12.0",
    "react-content-loader": "^4.3.4",
    "react-dom": "^16.12.0",
    "react-intl": "^3.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-promise-middleware": "^6.1.2",
    "redux-thunk": "^2.3.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/stories/*",
      "!src/entry.js",
      "!src/entry-dev.js",
      "!src/frontend-components-copies/*"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "axios-mock-adapter": "^1.17.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "connect-history-api-fallback": "^1.5.0",
    "css-loader": "^2.1.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-react": "^7.18.0",
    "fetch-mock": "^7.7.3",
    "file-loader": "^1.1.11",
    "git-revision-webpack-plugin": "^3.0.4",
    "html-replace-webpack-plugin": "^2.5.6",
    "html-webpack-plugin": "^3.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "koa-connect": "^2.0.1",
    "mini-css-extract-plugin": "^0.4.0",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.3",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "sass-loader": "^7.3.1",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "stylelint": "^9.3.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-scss": "^3.13.0",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-serve": "^1.0.2",
    "whatwg-fetch": "^3.0.0",
    "write-file-webpack-plugin": "^4.5.1",
    "xhr-mock": "^2.5.1"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --config config/prod.webpack.config.js",
    "test": "jest --coverage",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint config src",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelint.json",
    "prod": "NODE_ENV=production WEBPACK_SERVE=production webpack-serve --config config/dev.webpack.config.js",
    "server:ctr": "node src/server/generateServerKey.js",
    "start": "BETA=true WEBPACK_SERVE=development webpack-serve --config config/dev.webpack.config.js",
    "travis:build": "webpack --config config/test.webpack.config.js",
    "travis:verify": "npm-run-all travis:build lint test",
    "verify": "npm-run-all build lint test"
  }
}
