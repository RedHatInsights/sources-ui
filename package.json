{
  "name": "sources-ui",
  "version": "1.1.0",
  "private": false,
  "insights": {
    "appname": "sources"
  },
  "dependencies": {
    "@data-driven-forms/pf4-component-mapper": "^2.4.8",
    "@data-driven-forms/react-form-renderer": "^2.4.8",
    "@patternfly/react-core": "^3.158.3",
    "@patternfly/react-icons": "^3.15.17",
    "@patternfly/react-table": "^2.28.47",
    "@redhat-cloud-services/frontend-components": "2.0.6",
    "@redhat-cloud-services/frontend-components-notifications": "2.0.4",
    "@redhat-cloud-services/frontend-components-sources": "2.0.8",
    "@redhat-cloud-services/frontend-components-utilities": "2.0.4",
    "@sentry/minimal": "5.17.0",
    "axios": "0.19.2",
    "deep-object-diff": "^1.1.0",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-intl": "^4.6.9",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-promise-middleware": "^6.1.2",
    "redux-thunk": "^2.3.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/stories/*",
      "!src/entry.js",
      "!src/entry-dev.js",
      "!src/frontend-components-copies/*"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "axios-mock-adapter": "^1.18.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^3.6.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-react": "^7.20.0",
    "file-loader": "^6.0.0",
    "git-revision-webpack-plugin": "^3.0.6",
    "html-replace-webpack-plugin": "^2.5.6",
    "html-webpack-plugin": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^1.0.0",
    "style-loader": "^1.2.1",
    "stylelint": "^13.6.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.17.2",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "whatwg-fetch": "^3.0.0",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --config config/prod.webpack.config.js",
    "test": "jest --coverage",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint config src",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelint.json",
    "prod": "NODE_ENV=production webpack-dev-server --config config/dev.webpack.config.js",
    "server:ctr": "node src/server/generateServerKey.js",
    "start": "BETA=true webpack-dev-server --config config/dev.webpack.config.js",
    "travis:build": "webpack --config config/test.webpack.config.js",
    "travis:verify": "npm-run-all travis:build lint test",
    "verify": "npm-run-all build lint test"
  }
}
